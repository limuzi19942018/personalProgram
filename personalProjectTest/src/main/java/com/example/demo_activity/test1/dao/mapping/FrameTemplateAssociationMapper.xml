<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo_activity.test1.dao.FrameTemplateAssociationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo_activity.test1.model.FrameTemplateAssociation">
        <id column="id" property="id" />
        <result column="report_desc_id" property="reportDescId" />
        <result column="content_id" property="contentId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="status" property="status" />
        <result column="content_name" property="contentName" />
        <result column="template_combination_id" property="templateCombinationId" />
    </resultMap>

    <update id="updateOldData">
        update frame_template_association   set `status`=0
        where content_name is null and template_combination_id is  null;
    </update>
</mapper>
